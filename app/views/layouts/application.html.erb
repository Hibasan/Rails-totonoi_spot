<!DOCTYPE html>
<html>
  <head>
    <title>TotoSupo</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= link_to 'レターオープン' , letter_opener_web_path %>
    <%= link_to "admin画面", rails_admin_path %>
    <%= link_to 'トップ' , root_path %>
    <%= link_to 'このサイトについて' , about_path %>
    <%= link_to '検索ページ' , facilities_search_path %>
    <% if user_signed_in? %>
      <%= link_to 'ログアウト' , destroy_user_session_path ,method: :delete%>
      <%= link_to 'マイページ' , user_path(1) %>
      <% else %>
      <%= link_to 'ログイン' , new_user_session_path %>
      <%= link_to 'アカウント登録' , new_user_registration_path %>
    <% end %>



    <br><br>
    <h3>ヘッダー</h3>
    <%= yield %>
    <h3>フッター</h3>
    <br><br>
    <h4>Project by Hibasan2105</h4>
  </body>
</html>
